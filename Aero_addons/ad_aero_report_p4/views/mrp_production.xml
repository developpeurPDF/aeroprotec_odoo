<?xml version="1.0" encoding="utf-8"?>
<odoo>
      <data>
        <record id="mrp_production_workorder_tree_editable_view_inherit" model="ir.ui.view">
			<field name="name">mrp.production.work.order.tree.editable.inherit</field>
			<field name="model">mrp.workorder</field>
			<field name="inherit_id" ref="mrp.mrp_production_workorder_tree_editable_view"/>
			<field name="arch" type="xml">
                <xpath expr="//field[@name='name']" position="after">
                    <field name="worksheet_ids" invisible="1"/>
                </xpath>
            </field>
        </record>
         <record id="mrp_production_workorder_form_editable_view_inherit" model="ir.ui.view">
			<field name="name">mrp.production.work.order.form.editable.inherit</field>
			<field name="model">mrp.production</field>
			<field name="inherit_id" ref="mrp.mrp_production_form_view"/>
			<field name="arch" type="xml">
               <xpath expr="//header" position="inside">
          
         <button name="action_check" 
        string="OF" 
        type="object"
        class="btn-primary"
        icon="fa-print" invisible="1"/>   
      
        </xpath>
            </field>
        </record>
      </data>
</odoo>