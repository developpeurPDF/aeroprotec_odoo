<odoo>
    <data>
        <record id="paperformat_reportt" model="report.paperformat">
            <field name="name">Ordre de fabrication</field>
            <field name="default" eval="True" />
            <field name="format">A4</field>
            <field name="page_height">0</field>
            <field name="page_width">0</field>
            <field name="orientation">Portrait</field>
            <field name="margin_top">5</field>
            <field name="margin_bottom">20</field>
            <field name="margin_left">4</field>
            <field name="margin_right">6</field>
            <field name="header_line" eval="False" />
            <field name="header_spacing">45</field>
            <field name="dpi">90</field>
        </record>
         <record id="ordre_de_fabrication_report" model="ir.actions.report">
           <field name="name">OF COMPLET</field>
            <field name="model">mrp.production</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">ad_aero_report_p4.mrp_ordre_de_fabrication_report</field>
            <field name="report_file">mrp_ordre_de_fabrication_report</field>
            <field name="print_report_name">'Ordre de fabrication complet - %s' % (object.name)</field>
            <field name="binding_model_id" ref="model_mrp_production"/>
            <field name="binding_type">report</field>
            <field name="paperformat_id" ref="ad_aero_report_p4.paperformat_reportt"/>
        </record>
    </data>
</odoo>